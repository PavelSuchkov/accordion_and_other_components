ock');\r\n\r\n    useEffect(() => {\r\n        console.log('Rerender 1234')\r\n        setInterval(() => {\r\n            console.log('Rerender Component')\r\n            let date = new Date();\r\n            setSeconds(date.getSeconds())\r\n            setMinutes(date.getMinutes())\r\n            setHours(date.getHours())\r\n        }, 1000)\r\n    } )\r\n\r\n\r\n    return <>\r\n        <h1>\r\n            {hours}-:-{minutes}-:-{seconds}\r\n        </h1>\r\n    </>\r\n\r\n}","C:\\Users\\admin\\WorkSpace\\Study\\Accordion\\ts-app\\src\\components\\Clock\\Clock.stories.tsx",["205"],"C:\\Users\\admin\\WorkSpace\\Study\\Accordion\\ts-app\\src\\components\\Clock\\Clock.tsx",["206","207"],"C:\\Users\\admin\\WorkSpace\\Study\\Accordion\\ts-app\\src\\components\\Clock\\DigitalClockView.tsx",[],"C:\\Users\\admin\\WorkSpace\\Study\\Accordion\\ts-app\\src\\components\\Clock\\AnalogClockView.tsx",[],{"ruleId":"208","replacedBy":"209"},{"ruleId":"210","replacedBy":"211"},{"ruleId":"212","severity":1,"message":"213","line":3,"column":9,"nodeType":"214","messageId":"215","endLine":3,"endColumn":22},{"ruleId":"212","severity":1,"message":"216","line":9,"column":9,"nodeType":"214","messageId":"215","endLine":9,"endColumn":15},{"ruleId":"212","severity":1,"message":"217","line":9,"column":17,"nodeType":"214","messageId":"215","endLine":9,"endColumn":27},{"ruleId":"212","severity":1,"message":"218","line":15,"column":10,"nodeType":"214","messageId":"215","endLine":15,"endColumn":28},{"ruleId":"212","severity":1,"message":"219","line":15,"column":30,"nodeType":"214","messageId":"215","endLine":15,"endColumn":51},{"ruleId":"212","severity":1,"message":"220","line":1,"column":16,"nodeType":"214","messageId":"215","endLine":1,"endColumn":24},{"ruleId":"208","replacedBy":"221"},{"ruleId":"210","replacedBy":"222"},{"ruleId":"208","replacedBy":"223"},{"ruleId":"210","replacedBy":"224"},{"ruleId":"208","replacedBy":"225"},{"ruleId":"210","replacedBy":"226"},{"ruleId":"208","replacedBy":"227